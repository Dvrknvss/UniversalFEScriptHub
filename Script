                TotalHats1 += 1
            end
            for i,v in pairs(HatsName1) do
                if game.Players.LocalPlayer.Character ~= nil then
                    if game.Players.LocalPlayer.Character:FindFirstChild(v) then
                    UserHats1 += 1
                    end
                end
            end
            if UserHats1 == TotalHats1 then
                UserHats1 = 0
                TotalHats1 = 0
                OrionLib:MakeNotification({Name = "Sucefully!", Content = "All hats have been equipeds!", Image = "rbxassetid://4483345998", Time = 3})
