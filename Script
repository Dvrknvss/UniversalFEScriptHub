    end})


    SectionLP:AddButton({Name = "Normal Reset", Callback = function()
        game.Players.LocalPlayer.Character.Humanoid.Health = 0
        wait(4)
    end})

    SectionLP:AddButton({Name = "FE Hard Reset", Callback = function()
        if game.PlaceId == 5100950559 and _G.justgrassR == true then
            game.Players:Chat('-gr')
            OrionLib:MakeNotification({Name = "Sucefully!", Content = "Just Grass mode reset!", Image = "rbxassetid://4483345998", Time = 2})
        else
            if _G.reset == false then
                _G.reset = true
                game.Players.LocalPlayer.Character:BreakJoints()
                wait(0.2)
                game.Players.LocalPlayer.Character:Destroy()
                OrionLib:MakeNotification({Name = "Resetting...", Content = "Wait a few seconds, reset in progress.", Image = "rbxassetid://4483345998", Time = 6.5})
                wait(6.5)
                _G.reset = false
            else
                OrionLib:MakeNotification({Name = "Warning!", Content = "Reset already in progress!", Image = "rbxassetid://4483345998", Time = 2})
            end
        end
    end})


    SectionConfig:AddBind({Name = "Configure FE Hard Reset Keybind", Default = Enum.KeyCode.R, Hold = false, Callback = function()

        if game.PlaceId == 5100950559 and _G.justgrassR == true then
            game.Players:Chat('-gr')
            OrionLib:MakeNotification({Name = "Sucefully!", Content = "Just Grass mode reset!", Image = "rbxassetid://4483345998", Time = 2})
        else
            if _G.reset == false then
                _G.reset = true
                game.Players.LocalPlayer.Character:BreakJoints()
                wait(0.2)
                game.Players.LocalPlayer.Character:Destroy()
                OrionLib:MakeNotification({Name = "Resetting...", Content = "Wait a few seconds, reset in progress.", Image = "rbxassetid://4483345998", Time = 6.5})
                wait(6.5)
                _G.reset = false
            else
                OrionLib:MakeNotification({Name = "Warning!", Content = "Reset already in progress!", Image = "rbxassetid://4483345998", Time = 2})
            end
        end
    end})


    -- Teleport

    _G.plr = "Select player to teleport"
    local drop = SectionTeleport:AddDropdown({Name = "Select Player Here", Default = "Select player to teleport", Options = listplr, Callback = function(Value)
        _G.plr = Value
    end})
    SectionTeleport:AddButton({Name = "Teleport to selected player", Callback = function()
        if _G.plr == "Select player to teleport" then
            OrionLib:MakeNotification({Name = "Error!", Content = "Player = nil!", Image = "rbxassetid://4483345998", Time = 3})
        else 
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players[_G.plr].Character.HumanoidRootPart.CFrame
        end
    end})  
    SectionTeleport:AddButton({Name = "Refresh Dropdown", Callback = function()
        local listplr = {}
        for i,v in pairs(game:GetService("Players"):GetPlayers()) do
                table.insert(listplr, v.Name)
        end
        drop:Refresh(listplr ,true)
    end})


    SectionGenTeleport:AddButton({Name = "Copy coordinates", Callback = function()
        OrionLib:MakeNotification({Name = "Sucefully!", Content = "Cords copied to clipboard", Image = "rbxassetid://4483345998", Time = 3})
        pcall(function()
            setclipboard(tostring(game.Players.LocalPlayer.Character.HumanoidRootPart.Position))  
        end)
    end})
    SectionGenTeleport:AddButton({Name = "Generate Teleport Script", Callback = function()
        OrionLib:MakeNotification({Name = "Sucefully!", Content = "Script copied to clipboard", Image = "rbxassetid://4483345998", Time = 3})
        pcall(function()
            local pos = tostring(game.Players.LocalPlayer.Character.HumanoidRootPart.Position)
            setclipboard("game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new("..pos..")")  
        end)
    end})


    _G.timetotween = 3 

    SectionGenTeleport:AddSlider({Name = "Tween Time", Min = 1, Max = 15, Default = 3, Color = Color3.fromRGB(154, 220, 255), Increment = 1, ValueName = " ", Callback = function(Value)
        _G.timetotween = Value
    end})
    SectionGenTeleport:AddButton({Name = "Generate Tween Teleport Script", Callback = function()
        OrionLib:MakeNotification({Name = "Sucefully!", Content = "Tween script copied to clipboard", Image = "rbxassetid://4483345998", Time = 3})
            local pos = tostring(game.Players.LocalPlayer.Character.HumanoidRootPart.Position)
            setclipboard('tweenService, tweenInfo = game:GetService("TweenService"), TweenInfo.new('.._G.timetotween..', Enum.EasingStyle.Linear)\ntweenService:Create(game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart, tweenInfo, {CFrame = CFrame.new('..pos..')}):Play()')  
    end})



    SectionTpGames:AddButton({Name = "Rejoin In This Game", Callback = function()
        game:GetService("TeleportService"):Teleport(game.PlaceId, player)
    end})

    _G.game = "Select game to teleport"
    SectionTpGames:AddDropdown({Name = "Select Game Here", Default = "Select game to teleport", Options = {"Just Grass", "Natural Disaster", "Prison Life", "Life In Paradise (Force R6)", "White Room (Force R15)", "MIC UP", "Pick A Side", "Rate My Avatar", "Ro-Chat Spatial Voice"}, Callback = function(Value)
        _G.game = Value
    end})

    SectionTpGames:AddButton({Name = "Teleport To Selected Game", Callback = function()
        if _G.game == "Select game to teleport" then
            OrionLib:MakeNotification({Name = "Warning!", Content = "Select game first to teleport!", Image = "rbxassetid://4483345998", Time = 3})
        elseif _G.game == "Just Grass" then
            game:GetService("TeleportService"):Teleport(5100950559, player)
        elseif _G.game == "Natural Disaster" then
            game:GetService("TeleportService"):Teleport(189707, player)
        elseif _G.game == "Prison Life" then
            game:GetService("TeleportService"):Teleport(155615604, player)
        elseif _G.game == "Life In Paradise (Force R6)" then
            game:GetService("TeleportService"):Teleport(1662219031, player)
        elseif _G.game == "White Room (Force R15)" then
            game:GetService("TeleportService"):Teleport(4765805939, player)
        elseif _G.game == "MIC UP" then
            game:GetService("TeleportService"):Teleport(6884319169, player)
        elseif _G.game == "Pick A Side" then
            game:GetService("TeleportService"):Teleport(663655429, player)
        elseif _G.game == "Rate My Avatar" then
            game:GetService("TeleportService"):Teleport(6708206173, player)
        elseif _G.game == "Ro-Chat Spatial Voice" then
            game:GetService("TeleportService"):Teleport(6154671407, player)
        end
    end})

    SectionScripts:AddButton({Name = "Infinite Yield", Callback = function()
        OrionLib:MakeNotification({Name = "Script executed", Content = "Script credits: EdgeIY", Image = "rbxassetid://4483345998", Time = 3})
        wait(3)
        loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
    end})
    SectionScripts:AddButton({Name = "Shed's Bypasser V3", Callback = function()
        OrionLib:MakeNotification({Name = "Script executed", Content = "Script credits: Shed", Image = "rbxassetid://4483345998", Time = 3})
        wait(3)
        loadstring(game:HttpGet("https://the-shed.xyz/roblox/scripts/ChatBypass", true))()
    end})
    SectionScripts:AddButton({Name = "R15 Emotes", Callback = function()
        OrionLib:MakeNotification({Name = "Script executed", Content = "Script credits: Gi#7331", Image = "rbxassetid://4483345998", Time = 3})
        OrionLib:MakeNotification({Name = "Toggleable:", Content = "Press Comma to Open", Image = "rbxassetid://4483345998", Time = 7})
        wait(3)
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Gi7331/scripts/main/Emote.lua", true))()
    end})


    SectionCredits:AddParagraph("Important Peoples","Creator and Owner: Dvrknvss#0688                                               Development: collorem#4664 (Retired)                                                                 Script Tester/Finder: On_Man#9106                                                    Script Contribuitons: Lego#9007 and NK#2022                                                  UI: Orion Library")
    SectionCredits:AddParagraph("Important!","Every script added here may contain bugs, it isn't our mistake.            All scripts works properly, some games doesn't support some FE. Don't worry if isn't working properly.")
    SectionCredits:AddButton({Name = "Click To Copy Discord Link ", Callback = function()
        OrionLib:MakeNotification({Name = "Join pls :)", Content = "Link copied to clipboard, https://discord.gg/McWdrB4wSs", Image = "rbxassetid://4483345998", Time = 3})
        setclipboard(tostring("https://discord.gg/McWdrB4wSs"))
    end})
    local market = game:GetService("MarketplaceService")
    local info = market:GetProductInfo(game.PlaceId, Enum.InfoType.Asset)
    plr = game:GetService'Players'.LocalPlayer
    
    SectionConfig:AddParagraph("Informations","UserName: "..game.Players.LocalPlayer.Name.."                                                                           User ID: "..game.Players.LocalPlayer.UserId.."                                                                Exploit: "..identifyexecutor().."                                                                             AccountAge: "..plr.AccountAge.."                                                                                        PlaceID: ".. game.PlaceId)
    OrionLib:Init()
